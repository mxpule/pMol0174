#include "pLabTools/pLabFreezerBox.h"
#include "pLabTools/pLabToolsFinderSlot.h"

pLabToolsFinderSlot::pLabToolsFinderSlot(pMolKernelInterface* p_interface,QTreeWidget *parent, int p_ixix, int p_boxNumber) : QTreeWidgetItem(parent)
{
  interface = p_interface;
  ixix = p_ixix;
  boxNumber = p_boxNumber;
};

pLabToolsFinderSlot::pLabToolsFinderSlot(pMolKernelInterface* p_interface,QTreeWidgetItem *parent, int p_ixix,  int p_boxNumber) : QTreeWidgetItem(parent)
{
  interface = p_interface;
  ixix = p_ixix;
  boxNumber = p_boxNumber;
};

void pLabToolsFinderSlot::doubleClicked(int column)
{
  if (boxNumber < 1) return;
// pLabFreezerBox* box = new pLabFreezerBox(interface, boxNumber, ixix);
 pLabFreezerBox* box = new pLabFreezerBox(interface, boxNumber, ixix);
};
